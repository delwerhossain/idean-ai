{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(npm install:*)",
      "Bash(pnpm dev:*)",
      "Bash(test:*)",
      "WebFetch(domain:localhost)",
      "Bash(curl:*)",
      "Bash(pnpm install:*)",
      "Bash(git commit:*)",
      "Bash(pnpm run lint:fix:*)",
      "mcp__playwright__browser_navigate",
      "mcp__playwright__browser_take_screenshot",
      "Bash(git add:*)",
      "Bash(git push:*)",
      "mcp__playwright__browser_install",
      "mcp__playwright__browser_close",
      "WebSearch",
      "Bash(npx shadcn@latest add:*)",
      "Bash(pnpm add:*)",
      "Bash(pnpm build:*)",
      "Bash(timeout 60s pnpm run type-check)",
      "mcp__playwright__browser_resize",
      "mcp__playwright__browser_console_messages",
      "Bash(find:*)",
      "Bash(timeout 30s pnpm dev)",
      "Bash(pnpm run lint:*)",
      "Bash(pnpm remove:*)",
      "Bash(grep:*)",
      "Bash(if [ -n \"$MONGODB_URI\" ])",
      "Bash(then echo \"MONGODB_URI is set\")",
      "Bash(else echo \"MONGODB_URI is not set\")",
      "Bash(fi)",
      "Bash(npx:*)",
      "Bash(rg:*)",
      "mcp__playwright__browser_type",
      "mcp__playwright__browser_snapshot",
      "mcp__playwright__browser_click",
      "mcp__playwright__browser_wait_for",
      "mcp__playwright__browser_fill_form",
      "Bash(timeout 30s pnpm run type-check)",
      "Bash(pnpm run:*)"
    ],
    "deny": [],
    "ask": [],
    "defaultMode": "acceptEdits",
    "additionalDirectories": [
      "*",
      "C:\\",
      "D:\\",
      "E:\\",
      "E:\\test\\other\\my\\webcloudor",
      "E:\\test\\other\\my\\webcloudor\\**",
      "F:\\",
      "G:\\",
      "/",
      "/home",
      "/usr",
      "/var",
      "/etc",
      "/opt",
      "/tmp",
      "~",
      ".",
      "..",
      "E:\\e\\test\\other\\my",
      "E:\\1code\\passionproject",
      "E:\\c\\1code\\passionproject",
      "E:\\e\\1code"
    ]
  },
  "settings": {
    "autoSave": true,
    "maxExecutionTimeSeconds": 3600,
    "defaultShell": "auto",
    "showNotifications": true,
    "logLevel": "debug",
    "enableWebRequests": true,
    "allowEditing": true,
    "defaultTimeoutSeconds": 600,
    "enableFileOperations": true,
    "enableNetworkOperations": true,
    "enableSystemOperations": true,
    "enableProcessOperations": true,
    "enableDatabaseOperations": true,
    "enableDockerOperations": true,
    "enableKubernetesOperations": true,
    "enableCloudOperations": true,
    "enableSecurityOperations": true,
    "enableAdminOperations": true,
    "allowUnrestrictedCommands": true,
    "allowAllPermissions": true,
    "allowRootAccess": true,
    "allowSystemModification": true,
    "allowNetworkAccess": true,
    "allowExternalConnections": true,
    "allowScriptExecution": true,
    "allowBinaryExecution": true,
    "allowCompilation": true,
    "allowInstallation": true,
    "allowDeletion": true,
    "allowEncryption": true,
    "allowAuthentication": true,
    "allowAuthorization": true,
    "bypassSecurityChecks": false,
    "enableExperimentalFeatures": true,
    "enableAdvancedFeatures": true,
    "enableDeveloperMode": true,
    "enableDebugMode": true,
    "enableVerboseLogging": true,
    "enablePerformanceMonitoring": true,
    "enableErrorReporting": true,
    "enableTelemetry": false,
    "maxMemoryUsage": "unlimited",
    "maxCpuUsage": "unlimited",
    "maxDiskUsage": "unlimited",
    "maxNetworkUsage": "unlimited",
    "maxFileSize": "unlimited",
    "maxDirectoryDepth": "unlimited",
    "maxConcurrentOperations": "unlimited"
  }
}